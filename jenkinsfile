pipeline {

    agent  any 
    parameters {
        choice(name: 'VERSION', choices: ['1.1','1.2','1.3'])
    }
    stages {
        stage ('checkout '){
            steps{
                checkout scm
            }
            
        }
        stage('Example') {
            steps {
                script {
                    echo scm.branches[0].name
                         if (env.BRANCH_NAME == 'master') {
                                    echo 'I only execute on the master branch'
                                } else {
                                    echo 'I execute elsewhere'
                                }
                }
                   
            }
            
        }

        
        stage('test') {
            steps {
             echo "I am master branch"
            }

            when {
            branch 'master'
            }
        }

    }
}