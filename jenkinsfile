node {

    parameters {
        choice(name: 'VERSION', choices: ['1.1','1.2','1.3'])
    }
        stages {
        stage ('checkout '){
            checkout scm
        }
        stage('Example') {
            if (params.BRANCH_NAME == 'master') {
                echo 'I only execute on the master branch'
            } else {
                echo 'I execute elsewhere'
            }
        }

        stage('Example') {
            if (params.BRANCH_NAME == 'master') {
                echo 'I only execute on the master branch'
            } else {
                echo 'I execute elsewhere'
            }
        }
    }
}